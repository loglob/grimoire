		"Util.ts",
		"Games/IGame.ts",
		"Games/DnD5e.ts",
		"Games/Goedendag.ts",
		"Games/Pf2e.ts",
		"UI/Common.ts",
		"UI/Cards.ts",
		"UI/Details.ts",
		"UI/Index.ts",
		"UI/Table.ts",
		"UI/List.ts"
	]
}