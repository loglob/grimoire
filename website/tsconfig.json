{
	"compilerOptions": {
		"allowUnreachableCode": false,
		"alwaysStrict": true,
		"noImplicitAny": true,
		"noImplicitOverride": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"noUncheckedIndexedAccess": false,
		"noUnusedLocals": true,
		"preserveConstEnums": true,
		"removeComments": true,
		"sourceMap": true,
